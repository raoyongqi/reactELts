{
  "main": "./build/main/main.js",
  "private": "true",
  "scripts": {
    "start": "npm-run-all --parallel start:main start:main:watch start:renderer",
    "start:main": "cross-env NODE_ENV=development  electron .",
    "start:main:watch": "tsc -p ./src/main/tsconfig.json ",
    "start:renderer": "vite -c ./src/renderer/vite.config.js --host"
  },
  "include": [
    "src/**/*.ts"
  ],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "nodemon": "^3.1.7",
    "vite": "^6.0.3"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^33.2.1",
    "electron-devtools-installer": "^3.2.0",
    "log4js": "^6.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
